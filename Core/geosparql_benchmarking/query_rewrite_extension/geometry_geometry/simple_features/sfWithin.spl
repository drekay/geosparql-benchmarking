#Query 1 - sfWithin:
PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX dataset: <http://geographica.di.uoa.gr/dataset/>

SELECT ?s1 ?s2
WHERE {
	GRAPH dataset:geonames {
		?geometry1 a geo:Geometry .
	}
	
	GRAPH dataset:gag {
		?geometry2 a geo:Geometry .
	}
	
	?geometry1 geo:sfWithin ?geometry2 .
}