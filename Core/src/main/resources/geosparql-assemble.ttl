@prefix :      <http://example.org/geosparql#> .
@prefix tdb:   <http://jena.hpl.hp.com/2008/tdb#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ja:    <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

<#dataset> rdf:type       ja:RDFDataset ;
    ja:namedGraph       :gag-model-inf ;
    ja:namedGraph       :lgd-model-inf ;
    ja:namedGraph       :geonames-model-inf ;
    ja:namedGraph       :hotspots-model-inf ;
    ja:namedGraph       :clc-model-inf ;
    ja:namedGraph       :dbpedia-model-inf .

#TDB dataset on disk.
:tdb-dataset rdf:type tdb:DatasetTDB ;
    tdb:location  "geosparql_jena_tdb" .

#Create a RDFS Factory as re-used.
:rdfsFactory rdf:type ja:ReasonerFactory;
    ja:reasonerURL <http://jena.hpl.hp.com/2003/RDFSExptRuleReasoner>;
    ja:schema :geoRDFS .

#RDFS Schema loaded as memory model.
:geoRDFS rdf:type ja:MemoryModel ;
    ja:content [ja:externalContent <file:///geosparql_vocab_all.rdf>] .

#Specify named graph in the dataset.
:gag-graph rdf:type tdb:GraphTDB ;
    tdb:dataset :tdb-dataset ;
    tdb:graphName <http://geographica.di.uoa.gr/dataset/gag> .

#Inference model combining graph and reasoner.
:gag-model-inf rdf:type ja:InfModel ;
    ja:baseModel :gag-graph ;
    ja:reasoner :rdfsFactory.

#:lgd-graph rdf:type tdb:GraphTDB ;
#    tdb:dataset :tdb-dataset ;
#    tdb:graphName <http://geographica.di.uoa.gr/dataset/lgd> .

#:lgd-model-inf rdf:type ja:InfModel ;
#    ja:baseModel :lgd-graph ;
#    ja:reasoner :rdfsFactory .

#Does the name URI need to go against the inference model? Don't know and no answer.
:lgd-graph rdf:type tdb:GraphTDB ;
    tdb:dataset :tdb-dataset .

:lgd-model-inf rdf:type  ja:InfModel ;
    ja:graph :lgd-graph ;
    ja:graphName <http://geographica.di.uoa.gr/dataset/lgd> ;
    ja:reasoner :rdfsFactory.

:geonames-graph rdf:type tdb:GraphTDB ;
    tdb:dataset :tdb-dataset ;
    tdb:graphName <http://geographica.di.uoa.gr/dataset/geonames> .

:geonames-model-inf rdf:type  ja:InfModel ;
    ja:baseModel :geonames-graph ;
    ja:reasoner :rdfsFactory.

:hotspots-graph rdf:type tdb:GraphTDB ;
    tdb:dataset :tdb-dataset ;
    tdb:graphName <http://geographica.di.uoa.gr/dataset/hotspots> .

:hotspots-model-inf rdf:type ja:InfModel ;
    ja:baseModel :hotspots-graph ;
    ja:reasoner :rdfsFactory.

:clc-graph rdf:type tdb:GraphTDB ;
    tdb:dataset :tdb-dataset ;
    tdb:graphName <http://geographica.di.uoa.gr/dataset/clc> .

:clc-model-inf a ja:InfModel ;
    ja:baseModel :clc-graph ;
    ja:reasoner :rdfsFactory.

:dbpedia-graph rdf:type tdb:GraphTDB ;
    tdb:dataset :tdb-dataset ;
    tdb:graphName <http://geographica.di.uoa.gr/dataset/dbpedia> .

:dbpedia-model-inf a ja:InfModel ;
    ja:baseModel :dbpedia-graph ;
    ja:reasoner :rdfsFactory .
