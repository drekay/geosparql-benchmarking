#Query 8 - sfWithin:
PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX dataset: <http://geographica.di.uoa.gr/dataset/>

SELECT ?s1 ?s2
WHERE {
	GRAPH dataset:geonames {
		?feature1 a geo:Feature .
	}
	
	GRAPH dataset:gag {
		?feature2 a geo:Feature .
	}
	
	?feature1 geo:sfWithin ?feature2 .
}